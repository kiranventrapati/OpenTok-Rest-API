<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
 "-//Hibernate/Hibernate Mapping DTD//EN"
 "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.openTok.model.VideoCall" table="Video_Call">
		<id name="id" column="ID">
			<generator class="increment" />
		</id>
		
		<property name="caller" column="Caller_Id"  />
		<property name="receiver" column="Receiver_Id" />
		<property name="sessionId" column="Session_Id" />
		<property name="callerToken" column="Caller_Token" type="string" length="1000"/>
		<property name="receiverToken" column="Receiver_Token" />
		<property name="whenCreated" column="whenCreated" />
	</class>
</hibernate-mapping>
          

